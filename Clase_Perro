public class Perro {
    private String Nombre;
    private String Raza;
    private int Edad;

    public void inicializar(String n, String r, int e) {
        Nombre = n;
        Raza = r;
        Edad = e;
    }

    public String datos() {
        return "Nombre: " + Nombre + " Raza:" + Raza + " AÃ±os:" + Edad;
    }
    public String ladrar(){
        return "Guau,quau";
    }
}
