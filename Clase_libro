public class Libro {
    private String titulo;
    private String autor;
    private int añoPublicacion;

    public void init(String t, String a, int ap){
        titulo = t;
        autor = a;
        añoPublicacion = ap;
    }
    public String datosLibro(){
        return "Libro: "+titulo+" Autor: "+autor+" Año de publicación: "+añoPublicacion;
    }
}
