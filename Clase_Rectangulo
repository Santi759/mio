public class Rectangulo {
    private double Ancho;
    private double Altura;
    public void init(double an, double h){
        Ancho=an;
        Altura=h;
    }
    public double area(){
        return Ancho*Altura;
    }
    public double perimetro(){
        return (2*Ancho)+(2*Altura);
    }
}
